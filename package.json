{
  "name": "reloquent",
  "version": "1.3.2",
  "description": "Ask user configurable questions via read-line.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "mask": "yarn t test/mask",
    "test-build": "yarn test -e test-build",
    "b": "alamode src -o build -s",
    "doc": "doc -o README.md -d -a",
    "build": "yarn-s d b doc",
    "e": "alanode",
    "lint": "eslint .",
    "d": "yarn-s d1 externs types",
    "externs": "typal types/externs.js -e",
    "types": "typal types.js -c",
    "d1": "typal src/index.js -c -t types/index.xml,types/confirm.xml"
  },
  "files": [
    "build",
    "src",
    "types.js",
    "types"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/reloquent.git"
  },
  "keywords": [
    "readline",
    "ask",
    "questions",
    "stdin",
    "read",
    "input",
    "cli"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/reloquent/issues"
  },
  "homepage": "https://www.artd.eco",
  "devDependencies": {
    "alamode": "^3.5.1",
    "catchment": "^3.3.0",
    "documentary": "^1.37.0",
    "eslint-config-artdeco": "1.0.1",
    "forkfeed": "^1.1.1",
    "spawncommand": "^2.2.0",
    "yarn-s": "1.2.3",
    "zoroaster": "^4.3.0"
  },
  "dependencies": {
    "promto": "^1.2.0"
  }
}
