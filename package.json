{
  "name": "reloquent",
  "version": "1.1.0",
  "description": "Ask user configurable questions via read-line.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "b": "alamode src -o build -s",
    "d": "NODE_DEBUG=doc doc src/index.js -g",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "build": "yarn-s d b doc",
    "e": "node example",
    "example/single.js": "yarn e example/single.js",
    "example/string.js": "yarn e example/string.js",
    "example/questions.js": "yarn e example/questions.js",
    "lint": "eslint ."
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/reloquent.git"
  },
  "keywords": [
    "readline",
    "ask",
    "questions",
    "stdin",
    "read",
    "input"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/reloquent/issues"
  },
  "homepage": "https://github.com/artdecocode/reloquent#readme",
  "devDependencies": {
    "alamode": "1.5.1",
    "catchment": "3.1.1",
    "documentary": "1.20.1",
    "eslint-config-artdeco": "1.0.1",
    "forkfeed": "1.0.0",
    "spawncommand": "2.1.0",
    "yarn-s": "1.1.0",
    "zoroaster": "3.6.2"
  },
  "dependencies": {
    "promto": "1.0.1"
  }
}
