  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.js",
      "args": [
        "example/single.js"
      ],
      "env": {
        "BABEL_ENV": "debug",
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "args": [
        "test/spec/api.js",
        "-b"
      ],
      "env": {
        "BABEL_ENV": "debug",
        "ZOROASTER_TIMEOUT": "9999999"
      },
      "console": "integratedTerminal",
      // "skipFiles": [
      //   "<node_internals>/**/*.js"
      // ]
    }
  ]
}
